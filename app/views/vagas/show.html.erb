<%- model_class = Vaga -%>
<div class="page-header">
  <h1>Dados da vaga <%= @vaga.local%></h1>
</div>

<dl class="dl-horizontal">
  <dt><strong><%= model_class.human_attribute_name("Localização") %>:</strong></dt>
  <dd><%= @vaga.local %></dd>
  <dt><strong><%= model_class.human_attribute_name(:apartamento_id) %>:</strong></dt>
  <dd><%= @vaga.apartamento.local %></dd>

 </dl>
  
  <div class="page-header">
   <h1> Dados do Carro</h1>
  </div>

 
  <dl class="dl-horizontal">

  <dt><strong><%= model_class.human_attribute_name("Modelo") %>:</strong></dt>
  <dd><%= @vaga.modelo_carro %></dd>
  <dt><strong><%= model_class.human_attribute_name("Cor") %>:</strong></dt>
  <dd><%= @vaga.car_carro %></dd>
  <dt><strong><%= model_class.human_attribute_name("Placa") %>:</strong></dt>
  <dd><%= @vaga.placa_carro %></dd>
</dl>

<%= link_to t('.back', :default => t("Voltar")),
              vagas_path, :class => 'btn btn-default'  %>
<%= link_to t('.edit', :default => t("Editar")),
              edit_vaga_path(@vaga), :class => 'btn btn-default' %>
<%= link_to t('.destroy', :default => t("Apagar")),
              vaga_path(@vaga),
              :method => 'delete',
              :data => { :confirm => t('.confirm', :default => t("helpers.links.confirm", :default => 'Você tem certeza?')) },
              :class => 'btn btn-danger' %>
